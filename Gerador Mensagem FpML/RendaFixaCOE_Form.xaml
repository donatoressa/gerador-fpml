                    <TextBlock Text="Tipo Imposto" TextWrapping="WrapWithOverflow" Style="{StaticResource Labels}" />
                    <ComboBox Name="cbTipoImposto" IsEnabled="false">
                        <ComboBoxItem Content="IR"/>
                        <ComboBoxItem Content="IOF"/>
                    </ComboBox>
                </StackPanel>
                <StackPanel Name="panelValorImposto" Orientation="Horizontal" Height="30">
                    <TextBlock Text="Valor Imposto" TextWrapping="WrapWithOverflow" Style="{StaticResource Labels}"/>
                    <TextBox Name="tbValorImposto" Style="{StaticResource CaixaTexto}" IsEnabled="false"/>
                    <Button Name="AddRow" Content="+" HorizontalAlignment="Right" Width="20" Click="AddRow_Click" IsEnabled="false"/>
                </StackPanel>
                <StackPanel Name="panelListaImpostosLiquidacao" Orientation="Horizontal" Height="auto" Margin="0,10,0,0">
                    <DataGrid Name="dgImpostos" Width="300" Height="100" AutoGenerateColumns="False" ItemsSource="{Binding}" IsReadOnly="True" CanUserAddRows="True">
                        <DataGrid.Columns>
                            <DataGridTextColumn Header="Tipo Imposto" Width="130" Binding="{Binding TipoImposto}"/>
                            <DataGridTextColumn Header="Valor Imposto" Width="*" Binding="{Binding ValorImposto}"/>
                        </DataGrid.Columns>
                    </DataGrid>
                </StackPanel>
            </StackPanel>
            <StackPanel Name="Coluna4" Orientation="Vertical" Grid.Row="1" Grid.Column="3">
                
                
                <StackPanel Name="panelDemaisInfos" Orientation="Horizontal" Height="30">
                    <TextBlock Text="Demais Informações" FontWeight="Bold" Style="{StaticResource Labels}"/>
                </StackPanel>
                <StackPanel Name="panelDataEfetivaVencimento" Orientation="Horizontal" Height="30">
                    <TextBlock Text="Data Efetiva Vencimento"  TextWrapping="WrapWithOverflow" Style="{StaticResource Labels}"/>
                    <DatePicker Name="dpDataEfetivaVencimento" IsEnabled="False" />
                </StackPanel>
                <StackPanel Name="panelComprador" Orientation="Horizontal" Height="30">
                    <TextBlock Text="Comprador" TextWrapping="WrapWithOverflow" Style="{StaticResource Labels}"/>
                    <ComboBox Name="cbComprador" >
                        <ComboBoxItem Content="Parte"/>
                        <ComboBoxItem Content="Contraparte"/>
                    </ComboBox>
                </StackPanel>

                <StackPanel Name="panelCodigoFigura" Orientation="Horizontal" Height="30">
                    <TextBlock Text="Código Figura" TextWrapping="WrapWithOverflow" Style="{StaticResource Labels}"/>
                    <TextBox Name="tbCodigoFigura" Style="{StaticResource CaixaTexto}"/>
                </StackPanel>
                <StackPanel Name="panelCamaraRegistro" Orientation="Horizontal" Height="30">
                    <TextBlock Text="Câmara Registro" TextWrapping="WrapWithOverflow" Style="{StaticResource Labels}"/>
                    <ComboBox Name="cbNomeCamaraRegistro">
                        <ComboBoxItem Content="CETIP"/>
                        <ComboBoxItem Content="SELIC"/>
                        <ComboBoxItem Content="BMFBOVESPA"/>
                    </ComboBox>
                </StackPanel>
                <StackPanel Name="panelFuncionalOperador" Orientation="Horizontal" Height="30">
                    <TextBlock Text="Funcional Operador" TextWrapping="WrapWithOverflow" Style="{StaticResource Labels}"/>
                    <TextBox Name="tbFuncionalOperador" Style="{StaticResource CaixaTexto}"/>
                </StackPanel>
                <StackPanel Name="panelTipoFormaLiquidacao" Orientation="Horizontal" Height="30">
                    <TextBlock Text="Tipo de Forma de Liquidação" TextWrapping="WrapWithOverflow" Style="{StaticResource Labels}"/>
                    <ComboBox Name="cbTipoFormaLiquidacao" >
                        <ComboBoxItem Content="Financeira"/>
                        <ComboBoxItem Content="Física"/>
                    </ComboBox>
                </StackPanel>
                <StackPanel Name="panelCodInstrumentoFinanceiro" Orientation="Horizontal" Height="30">
                    <TextBlock Text="Código Instrumento Financeiro" TextWrapping="WrapWithOverflow" Style="{StaticResource Labels}"/>
                    <TextBox Name="tbCodIF" Style="{StaticResource CaixaTexto}" />
                </StackPanel>
                <StackPanel Name="panelCodTipoCondicaoMercadoLiquidacao" Orientation="Horizontal" Height="30">
                    <TextBlock Text="Código Tipo Condição Mercado Liquidação" TextWrapping="WrapWithOverflow" Style="{StaticResource Labels}"/>
                    <ComboBox Name="cbTipoCondicaoMercado" >
                        <ComboBoxItem Content="A mercado"/>
                        <ComboBoxItem Content="Na curva"/>
                        <ComboBoxItem Content="Sem condição"/>
                    </ComboBox>
                </StackPanel>
                <StackPanel Name="panelModalidade" Orientation="Horizontal" Height="30">
                    <TextBlock Text="Modalidade"  TextWrapping="WrapWithOverflow" Style="{StaticResource Labels}"/>
                    <ComboBox Name="cbModalidade" Height="30" VerticalAlignment="Top">
                        <ComboBoxItem Content="Capital investido garantido"/>
                        <ComboBoxItem Content="Perda limitada ao capital investido"/>
                    </ComboBox>
                </StackPanel>
                <StackPanel Name="panelTipoCondicaoResgateFinanceiro" Orientation="Horizontal" Height="30">
                    <TextBlock Text="Tipo Condição Resgate Financeiro" TextWrapping="WrapWithOverflow" Style="{StaticResource Labels}"/>
                    <ComboBox Name="cbCondicaoResgateFinanceiro">
                        <ComboBoxItem Content="Automatica"/>
                        <ComboBoxItem Content="Manual"/>
                        <ComboBoxItem Content="Não aplicável"/>
                    </ComboBox>
                </StackPanel>
                <StackPanel Name="panelCodOferta" Orientation="Horizontal" Height="30">
                    <TextBlock Text="Código Oferta" TextWrapping="WrapWithOverflow" Style="{StaticResource Labels}"/>
                    <TextBox Name="tbIdOferta" Style="{StaticResource CaixaTexto}"/>
                </StackPanel>
                <StackPanel Name="panelIndicadorOfertaPublica" Orientation="Horizontal" Height="30">
                    <TextBlock Text="Indicador Oferta Pública " TextWrapping="WrapWithOverflow" Style="{StaticResource Labels}"/>
                    <CheckBox Name="cbIdOfertaPublica" />
                </StackPanel>
                <StackPanel Name="panelPercentualCapitalGarantido" Orientation="Horizontal" Height="30">
                    <TextBlock Text="% Capital Garantido Cetip" TextWrapping="WrapWithOverflow" Style="{StaticResource Labels}"/>
                    <TextBox Name="tbPercentualCapitalGarantidoCetip" Style="{StaticResource CaixaTexto}"/>
                </StackPanel>
                <StackPanel Name="panelPercentualBaseAplicacaoCaixaOperacao" Orientation="Horizontal" Height="30">
                    <TextBlock Text="% Base Aplicacao Caixa Operacao" TextWrapping="WrapWithOverflow" Style="{StaticResource Labels}"/>
                    <TextBox Name="tbPercentualBaseAplicacaoCaixa" Style="{StaticResource CaixaTexto}"/>
                </StackPanel>
                <StackPanel Name="panelProtecaoContraProventos" Orientation="Horizontal" Height="30">
                    <TextBlock Text="Proteção Contra Proventos"  TextWrapping="WrapWithOverflow" Style="{StaticResource Labels}"/>
                    <ComboBox Name="cbProtecaoContraProventos" >
                        <ComboBoxItem Content="Ajustado pelo Emissor"/>
                        <ComboBoxItem Content="Ajustado pela Cetip"/>
                        <ComboBoxItem Content="Sem ajuste"/>
                    </ComboBox>
                </StackPanel>
                <StackPanel Name="panelIndVariacaoQuanto" Orientation="Horizontal" Height="30">
                    <TextBlock Text="Indicador Variação Quanto" TextWrapping="WrapWithOverflow" Style="{StaticResource Labels}"/>
                    <CheckBox Name="cbVariacaoQuanto" />
                </StackPanel>
                <StackPanel Name="panelValorInicialParidadeQuanto" Orientation="Horizontal" Height="30">
                    <TextBlock Text="Valor Inicial Paridade Quanto" TextWrapping="WrapWithOverflow" Style="{StaticResource Labels}"/>
                    <TextBox Name="tbValorParidadeQuanto" Style="{StaticResource CaixaTexto}"/>
                </StackPanel>
                <StackPanel Name="panelCancelamento" Orientation="Horizontal" Height="30">
                    <TextBlock Text="Dados Cancelamento" FontWeight="Bold" Style="{StaticResource Labels}"/>
                </StackPanel>
                <StackPanel Name="panelChaveEKMCancelamento" Orientation="Horizontal" Height="30">
                    <TextBlock Text="Chave EKM Cancelamento" Style="{StaticResource Labels}"/>
                    <TextBox Name="tbEKMCancelamento" Style="{StaticResource CaixaTexto}" />
                </StackPanel>

                <StackPanel Name="panelMotivoCancelamento" Orientation="Horizontal" >
                    <TextBlock Text="Descrição Motivo Cancelamento" TextWrapping="WrapWithOverflow" Style="{StaticResource Labels}"/>
                    <TextBox Name="tbMotivoCancelamento" Style="{StaticResource CaixaTexto}" TextWrapping="Wrap" Height="50"/>
                </StackPanel>
            </StackPanel>
            <StackPanel Name="Coluna5" Orientation="Vertical" Grid.Row="1" Grid.Column="4">
                <TextBox Name="taFpML" Margin="5" Height="700" Width="auto" TextWrapping="Wrap" ScrollViewer.VerticalScrollBarVisibility="Auto"/>
                <Button Name="btnLimpar" Content="Limpar" PreviewMouseLeftButtonDown="btnLimpar_PreviewMouseLeftButtonDown" Width="100" HorizontalAlignment="Right"/>
                <Button Name="btnGerarFpML" Content="Gerar FpML" PreviewMouseLeftButtonDown="btnGerarFpML_PreviewMouseLeftButtonDown" Width="100"  HorizontalAlignment="Right"/>
                <Button Name="btnExportarFpML" Content="Exportar FpML" PreviewMouseLeftButtonDown="btnExportarFpML_PreviewMouseLeftButtonDown" IsEnabled="False" Width="100" HorizontalAlignment="Right"/>
            </StackPanel>
        </Grid>
    </Grid>
</UserControl>
